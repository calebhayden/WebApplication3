@model WebApplication3.Models.EventViewModel
@{
                ViewBag.Title = "EventCreator";
                Layout = "~/Views/Shared/_Layout.cshtml";
}


@using (Html.BeginForm("NonClinical", "GetInvolved", FormMethod.Post))



<h2><Font color="Crimson">Create an Event</Font></h2>

<form>
    <label>Event Name</label>
    <div>
        @Html.TextBoxFor(Model => Model.EventSurgery)
    </div>


    <label>Location</label>
    <div>
        @Html.TextBoxFor(Model => Model.EventLocation)
    </div>

    <label>Date</label>
    <div>
        @Html.TextBoxFor(Model => Model.EventDate)
    </div>

    <label>Patient</label>
    <div>
        @Html.TextBoxFor(Model => Model.Patient)
    </div>

</form>

